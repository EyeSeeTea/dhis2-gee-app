{
    "name": "dhis2-gee-app",
    "description": "DHIS2 GEE App",
    "version": "1.1.0",
    "license": "GPL-3.0",
    "author": "EyeSeeTea team",
    "homepage": ".",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/eyeseetea/dhis2-gee-app.git"
    },
    "dependencies": {
        "@dhis2/app-runtime": "3.2.0",
        "@dhis2/d2-i18n": "1.1.0",
        "@dhis2/d2-i18n-extract": "1.0.8",
        "@dhis2/d2-i18n-generate": "1.2.0",
        "@dhis2/d2-ui-core": "7.3.3",
        "@dhis2/d2-ui-forms": "7.3.3",
        "@dhis2/ui": "7.2.1",
        "@eyeseetea/d2-api": "1.9.2",
        "@eyeseetea/d2-ui-components": "2.6.7",
        "@material-ui/core": "4.12.3",
        "@material-ui/icons": "4.11.2",
        "@material-ui/styles": "4.11.4",
        "async-promise-pool": "1.0.4",
        "axios": "0.21.4",
        "classnames": "2.3.1",
        "d2": "31.10.0",
        "d2-manifest": "1.0.0",
        "font-awesome": "4.7.0",
        "lodash": "4.17.21",
        "math-expression-evaluator": "1.3.8",
        "postcss-rtl": "1.7.3",
        "prop-types": "15",
        "purify-ts": "1.0.0",
        "purify-ts-extra-codec": "0.6.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-router-dom": "5.3.0",
        "react-scripts": "4.0.3",
        "styled-jsx": "4.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "craco-start": "craco start",
        "prebuild": "yarn localize && yarn test",
        "build-folder": "rm -rf build/ && d2-manifest package.json manifest.webapp && react-scripts build && yarn run manifest && cp -r i18n icon.png build",
        "build": "yarn build-folder && rm -f $npm_package_name.zip && cd build && zip --quiet -r ../$npm_package_name.zip *",
        "build-importer": "REACT_APP_DATA_IMPORTER=true yarn build && yarn manifest-importer && rm -f $npm_package_name-importer.zip && cd build && zip -r ../$npm_package_name-importer.zip *",
        "test": "jest --passWithNoTests",
        "lint": "eslint src cypress --ext .js,.jsx,.ts,.tsx",
        "eject": "react-scripts eject",
        "prettify": "prettier \"./**/*.{js,jsx,json,css,ts,tsx}\" --write",
        "extract-pot": "yarn d2-i18n-extract -p src/ -o i18n/",
        "localize": "yarn update-po && d2-i18n-generate -n dhis2-gee-app -p ./i18n/ -o ./src/locales/",
        "update-po": "yarn extract-pot && for pofile in i18n/*.po; do msgmerge --backup=off -U $pofile i18n/en.pot; done",
        "manifest": "d2-manifest package.json build/manifest.webapp",
        "manifest-importer": "yarn manifest -n 'DHIS2 GEE Data Importer App'",
        "cy:verify": "cypress verify",
        "cy:e2e:open": "NODE_ENV=test CYPRESS_E2E=true cypress open",
        "cy:e2e:run": "NODE_ENV=test CYPRESS_E2E=true cypress run --browser chrome",
        "prepare": "husky install",
        "validate-gee-api": "npx ts-node src/scripts/validate-gee-api.ts",
        "build-validate-gee-api-action": "ncc build .github/actions/validate-gee-api/main.ts -m -o .github/actions/validate-gee-api/"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn prettify && yarn lint && yarn update-po && yarn jest"
        }
    },
    "devDependencies": {
        "@actions/core": "1.9.1",
        "@babel/core": "7.15.5",
        "@babel/preset-typescript": "7.15.0",
        "@google/earthengine": "0.1.283",
        "@testing-library/cypress": "8.0.1",
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/react": "12.1.0",
        "@types/axios-mock-adapter": "1.10.0",
        "@types/classnames": "2.3.1",
        "@types/cryptr": "4.0.1",
        "@types/file-saver": "2.0.3",
        "@types/jest": "27.0.2",
        "@types/lodash": "4.14.173",
        "@types/material-ui": "0.21.9",
        "@types/math-expression-evaluator": "1.2.1",
        "@types/md5": "2.3.1",
        "@types/node": "16.9.6",
        "@types/react": "17.0.24",
        "@types/react-dom": "17.0.9",
        "@types/react-router-dom": "5.3.0",
        "@typescript-eslint/eslint-plugin": "4.31.2",
        "@typescript-eslint/parser": "4.31.2",
        "@vercel/ncc": "0.31.1",
        "@welldone-software/why-did-you-render": "6.2.1",
        "axios-mock-adapter": "1.20.0",
        "babel-core": "6.26.3",
        "babel-eslint": "10.1.0",
        "cmd-ts": "0.7.0",
        "craco": "0.0.3",
        "cypress": "8.4.1",
        "cypress-xpath": "1.6.2",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.6",
        "enzyme-to-json": "3.6.2",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-config-react-app": "6.0.0",
        "eslint-plugin-cypress": "2.12.1",
        "eslint-plugin-flowtype": "6.1.0",
        "eslint-plugin-import": "2.24.2",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.26.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "eslint-plugin-unused-imports": "1.1.4",
        "http-proxy-middleware": "2.0.1",
        "husky": "7.0.2",
        "jest": "27.2.1",
        "prettier": "2.4.1",
        "react-test-renderer": "17.0.2",
        "ts-jest": "27.0.5",
        "typescript": "4.4.3",
        "wait-on": "6.0.0"
    },
    "manifest.webapp": {
        "name": "DHIS2 GEE App",
        "description": "DHIS2 GEE App",
        "icons": {
            "48": "icon.png"
        },
        "developer": {
            "url": "https://www.eyeseetea.com/",
            "name": "EyeSeeTea team"
        },
        "activities": {
            "dhis": {
                "href": "*"
            }
        }
    }
}
